  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.1",
