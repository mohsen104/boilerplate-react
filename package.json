{
  "name": "boilerplate-react",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "biome lint --write .",
    "format": "biome format --write .",
    "check": "biome check .",
    "prepare": "husky",
    "msw:init": "msw init public/ --save"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.14",
    "@tanstack/react-query": "^5.90.2",
    "@types/node": "^24.7.0",
    "axios": "^1.12.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.3",
    "tailwindcss": "^4.1.14"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "msw": "^2.11.3",
    "typescript": "~5.8.3",
    "vite": "^7.1.9",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "biome format --write .",
      "biome lint --write ."
    ]
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
